{
  "name": "aws-liveness-turbo-modules",
  "version": "1.0.0",
  "description": "React Native Turbo Module for AWS Amplify Face Liveness Detection",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "src/",
    "android/",
    "ios/",
    "README.md",
    "README_FACE_LIVENESS.md",
    "README_FACE_LIVENESS_EN.md",
    "INTEGRATION_GUIDE.md",
    "SECURITY_GUIDE.md",
    "DOCUMENTATION.md"
  ],
  "keywords": [
    "react-native",
    "aws",
    "amplify",
    "face-liveness",
    "biometrics",
    "authentication",
    "kotlin",
    "turbo-module",
    "android",
    "ios"
  ],
  "author": "Iran Sousa <protesto.hc@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aws-liveness-turbo-modules.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/aws-liveness-turbo-modules/issues"
  },
  "homepage": "https://github.com/yourusername/aws-liveness-turbo-modules#readme",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "publish-package": "chmod +x scripts/publish.sh && ./scripts/publish.sh",
    "setup-manual": "node scripts/setup-manual.js"
  },
  "codegenConfig": {
    "name": "AwsLivenessTurboModules",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.aws.liveness.turbo.modules"
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-android": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-ios": "15.0.0-alpha.2",
    "@react-native/babel-preset": "0.76.0",
    "@react-native/eslint-config": "0.76.0",
    "@react-native/metro-config": "0.76.0",
    "@react-native/typescript-config": "0.76.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
